<launch>
  <!--sensors.launch launches the imu and both ousters-->	
	<include file="$(find ouster_backpack_bringup)/launch/sensors.launch" />

  <node name="cartographer_node" pkg="cartographer_ros"
      type="cartographer_node" args="
          -configuration_directory $(find ouster_backpack_bringup)/configuration_files
          -configuration_basename 2_ouster_3d.lua"
      output="screen">
    <remap from="/odom" to="/transformed_odom"/> 
    <remap from="/points2_1" to="/points_raw_1"/>
    <remap from="/points2_2" to="/points_raw_2"/>
    <remap from="/imu" to="/imu_raw"/>
  </node>

  <node name="cartographer_occupancy_grid_node" pkg="cartographer_ros"
      type="cartographer_occupancy_grid_node" args="-resolution 0.2" />

</launch>
