<launch>

  <node pkg="nodelet" type="nodelet" name="pcl_manager" args="manager" output="screen" />

  <!-- Run a passthrough filter to clean NaNs -->
  <node pkg="nodelet" type="nodelet" name="passthrough" args="load pcl/PassThrough pcl_manager" output="screen">
    <remap from="~input" to="/os1_cloud_node/points" />
    <rosparam>
      filter_field_name: z
      filter_limit_min: 0.01
      filter_limit_max: 2.0
      filter_limit_negative: False
    </rosparam>
  </node>


  <!--param name="robot_description"
    textfile="$(find li_pkg)/configs/qav500.urdf" />

  <node name="robot_state_publisher" pkg="robot_state_publisher"
    type="robot_state_publisher" /-->
  <node name="cartographer_node" pkg="cartographer_ros"
      type="cartographer_node" args="
          -configuration_directory $(find li_pkg)/configs
	  -configuration_basename gene_3D_1_Ouster.lua"
      output="screen">
    <remap from="/odom" to="/transformed_odom"/> 
    <remap from="/points2" to="/os1_cloud_node/points"/>
    <remap from="/imu" to="/gx5/imu/data"/>
  </node>

  <node name="cartographer_occupancy_grid_node" pkg="cartographer_ros"
      type="cartographer_occupancy_grid_node" args="-resolution 0.2" />

  <!--node pkg="octomap_server" name="husky_odom_publisher" type="husky_odom_pub" output="screen"/>
-->
<!--  <node name="rviz" pkg="rviz" type="rviz" required="true"
      args="-d $(find cartographer_ros)/configuration_files/H02_3d.rviz" />
-->
</launch>
